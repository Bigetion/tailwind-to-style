{
  "name": "tailwind-to-style",
  "version": "2.4.0",
  "description": "Convert tailwind classes to inline style",
  "main": "index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "index.min.js",
    "preflight.css",
    "README.md",
    "LICENSE"
  ],  "scripts": {
    "test": "jest --passWithNoTests",
    "build": "node build.js",
    "dev": "node build.js && nodemon --watch src -e js --exec 'npm run build'",
    "lint": "eslint src/**/*.js",
    "prepublishOnly": "npm run test && npm run build",
    "version": "git add -A",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bigetion/tailwind-to-style.git"
  },
  "keywords": [
    "tailwind",
    "tailwindcss",
    "css",
    "stylesheet"
  ],
  "author": "Bigetion",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Bigetion/tailwind-to-style/issues"
  },
  "homepage": "https://github.com/Bigetion/tailwind-to-style#readme",
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/preset-env": "^7.18.0",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "babel-jest": "^28.1.0",
    "eslint": "^8.16.0",
    "jest": "^28.1.0",
    "rollup": "^2.75.0",
    "rollup-plugin-terser": "^7.0.2"
  }
