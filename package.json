{
  "name": "tailwind-to-style",
  "version": "2.5.0",
  "description": "Convert tailwind classes to inline style",
  "type": "module",
  "main": "index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "index.min.js",
    "preflight.css",
    "README.md",
    "LICENSE"
  ],  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --passWithNoTests",
    "build": "node --experimental-vm-modules build-esm.js",
    "build:legacy": "node build-webpack.js",
    "build:simple": "node build.js",
    "dev": "node --experimental-vm-modules build-esm.js",
    "lint": "eslint src/**/*.js",
    "test:examples": "node examples/test/node-test.js",
    "test:browser": "start examples/test/browser-test.html",
    "prepublishOnly": "npm run test && npm run build",
    "version": "git add -A",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bigetion/tailwind-to-style.git"
  },
  "keywords": [
    "tailwind",
    "tailwindcss",
    "css",
    "stylesheet"
  ],
  "author": "Bigetion",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Bigetion/tailwind-to-style/issues"
  },
  "homepage": "https://github.com/Bigetion/tailwind-to-style#readme",
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "babel-jest": "^28.1.0",
    "babel-loader": "^10.0.0",
    "eslint": "^8.16.0",
    "jest": "^28.1.0",
    "rollup": "^2.75.0",
    "rollup-plugin-terser": "^7.0.2",
    "terser": "^5.43.1",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  }
}
