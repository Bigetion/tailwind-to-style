{
  "name": "tailwind-to-style",
  "version": "2.11.2",
  "description": "Zero-runtime Tailwind CSS-in-JS with styled components, variants system, SCSS-like nesting, and React integration",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "preflight.css",
    "README.md",
    "LICENSE",
    "plugins",
    "lib"
  ],
  "bin": {
    "twsx-cli": "./lib/twsx-cli.js"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --passWithNoTests",
    "build": "rollup -c rollup.config.js",
    "dev": "rollup -c rollup.config.js --watch",
    "lint": "eslint src/**/*.js",
    "test:examples": "node examples/test/node-test.js",
    "prepublishOnly": "npm run build",
    "version": "git add -A",
    "postversion": "git push && git push --tags",
    "prettify:src": "prettier --write \"src/**/*.js\"",
    "prettify:examples": "prettier --write \"examples/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bigetion/tailwind-to-style.git"
  },
  "keywords": [
    "tailwind",
    "tailwindcss",
    "css",
    "stylesheet"
  ],
  "author": "Bigetion",
  "license": "MIT",
  "funding": {
    "type": "individual",
    "url": "https://buymeacoffee.com/bigetion"
  },
  "bugs": {
    "url": "https://github.com/Bigetion/tailwind-to-style/issues"
  },
  "homepage": "https://github.com/Bigetion/tailwind-to-style#readme",
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.28.5",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "babel-jest": "^30.0.0",
    "babel-loader": "^10.0.0",
    "chokidar": "^4.0.1",
    "eslint": "^9.15.0",
    "jest": "^30.0.0",
    "prettier": "^3.5.3",
    "rollup": "^4.27.4",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-terser": "^7.0.2",
    "terser": "^5.43.1",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  }
}
